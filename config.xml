<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sp.qrcode" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>QRCodeReader</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-barcodescanner" version="0.7.4" />
    <plugin name="cordova-plugin-inappbrowser" version="3.0.0" />
    <plugin name="cordova-plugin-camera" version="4.0.3" />
    <plugin name="cordova-plugin-compat" version="1.2.0" />
    <plugin name="cordova-plugin-device" version="2.0.2" />
    <plugin name="cordova-plugin-media-capture" version="3.0.2" />
    <plugin name="cordova-plugin-file" version="6.0.1" />
    <plugin name="cordova-plugin-remote-injection" version="0.5.2" />
    <plugin name="cordova-plugin-statusbar" version="2.4.2" />
    <plugin name="cordova-plugin-splashscreen" version="5.0.2" />
</widget>
